{
  "name": "pomodoro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint --ext .js,jsx,ts,tsx,md src",
    "eslint:fix": "eslint --fix --ext .js,jsx,ts,tsx,md src",
    "prettier": "prettier --trailing-comma es5 \"src/**/*.{js,jsx,ts,tsx,css,less,json}\"",
    "prettier:fix": "prettier --trailing-comma es5 --write \"src/**/*.{js,jsx,ts,tsx,css,less,json}\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md}": [
      "yarn prettier",
      "yarn eslint"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/inter": "^5.0.16",
    "@types/react-router-dom": "^5.3.3",
    "framer-motion": "^10.16.16",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.20.1",
    "react-swipeable": "^7.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "prop-types": "15.8.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.7"
  }
